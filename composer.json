{
    "name": "bcremer/doctrine-mysql-spatial",
    "type": "library",
    "description": "Doctrine support for MySQL spatial types and functions",
    "keywords": ["orm", "dbal", "database", "mysql", "opengis", "gis", "spatial", "geometry", "geography"],
    "authors": [
        {
            "name": "Benjamin Cremer",
            "email": "bc@benjamin-cremer.de"
        },
        {
            "name": "Derek Lambert",
            "email": "dlambert@dereklambert.com"
        }
    ],
    "license": "MIT",
    "require": {
        "doctrine/orm": "^2.7",
        "creof/geo-parser": "~2.0",
        "creof/wkt-parser": "~2.0",
        "creof/wkb-parser": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Bcremer\\Spatial\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bcremer\\Spatial\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }

}
